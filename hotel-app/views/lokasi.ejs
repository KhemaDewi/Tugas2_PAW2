<div class="row d-flex align-items-center">
  <div class="col-md-6">
    <img src="<%= data.peta %>" class="w-100" alt="peta hotel" />
  </div>
  <div class="col-md-6">
    <h3><%= data.nama %></h3>
    <h5>Lokasi</h5>
    <ul class="list-unstyled">
      <li>Jalan: <%= data.alamat.jalan %></li>
      <li>Provinsi: <%= data.alamat.provinsi %></li>
      <li>Kode Pos: <%= data.alamat.kode_pos %></li>
      <li>Negara: <%= data.alamat.negara %></li>
    </ul>
  </div>
</div>
